#include <stdio.h>
#define TAM_ALUNO 15
#define TAM_PROFESSOR 3
#define TAM_DISCIPLINA 2

int main()
{

    struct Professor
    {
        char nomeProfessor[50], matriculaProfessor[50], titulacaoProfessor[50];
    };

    struct Aluno
    {
        char nomeAluno[50], matriculaAluno[50], disciplinaAluno[50];
        int idade;
    };

    struct Disciplina
    {
        char nomeDisciplina[50], codigoDisciplina[50], professorDisciplina[50];
        float cargaHorariaDisciplina;
    };

    struct Professor nomeProfessor[TAM_PROFESSOR], matriculaProfessor[TAM_PROFESSOR], titulacaoProfessor[TAM_PROFESSOR];
    struct Aluno nomeAluno[TAM_ALUNO], matriculaAluno[TAM_ALUNO], disciplinaAluno[TAM_ALUNO];
    struct Disciplina nomeDisciplina[TAM_DISCIPLINA], codigoDisciplina[TAM_DISCIPLINA], professorDisciplina[TAM_DISCIPLINA];

    return 0;
}